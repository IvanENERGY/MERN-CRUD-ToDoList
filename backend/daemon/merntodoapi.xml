<service>
	<id>merntodoapi.exe</id>
	<name>mernToDoApi</name>
	<description>This is service for merntodoApi </description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\user\Documents\MERNToDoList\backend\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
